<!DOCTYPE html>
<html>
  <head>
    <title>AirBnb</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    

    
    <%= javascript_importmap_tags %>
  </head>

  <body>
    <%= render"layouts/navbar" %>
